@model IEnumerable<OnlineWebShop.Models.AdminAccount>
    @if (Convert.ToString(Session["PermissionID"]) == "1")
    {
<div class="col-md-12"><button type="button" class="btn btn-success"><a href=" @Url.Action("InsertUser", "Modules")">Thêm mới</a></button></div>
<div class="col-md-12">
    <table class="table table-bordered table-hover table-striped">
        <thead>
            <tr>
                <th>Mã số</th>
                <th>Họ và tên</th>
                <th>Email</th>
                <th>Quyền hạn</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {

                <tr>
                    <td>@item.AdminID</td>
                    <td>@item.FullName</td>
                    <td>@item.Email</td>
                    <td>@item.Permission.PermissionName</td>
                    <td><button type="button" class="btn btn-warning"><a href=" @Url.Action("EditUser", "Modules", new { id = item.AdminID })" >Sửa</a></button> | <button type="button" class="btn btn-danger"><a href=" @Url.Action("DeleteUser", "Modules", new { id = item.AdminID })">Xóa</a></button></td>
                </tr>
            }

        </tbody>
    </table>
    </div>
    }
    else
    {
        <div class="col-md-12"><button type="button" class="btn btn-success" disabled><a href=" @Url.Action("InsertUser", "Modules")">Thêm mới</a></button></div>
        <div class="col-md-12">
            <table class="table table-bordered table-hover table-striped">
                <thead>
                    <tr>
                        <th>Mã số</th>
                        <th>Họ và tên</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {

                        <tr>
                            <td>@item.AdminID</td>
                            <td>@item.FullName</td>     
                                <td><button type="button" class="btn btn-warning" disabled><a href=" @Url.Action("EditUser", "Modules", new { id = item.AdminID })">Sửa</a></button> | <button type="button" class="btn btn-danger" disabled><a href=" @Url.Action("DeleteUser", "Modules", new { id = item.AdminID })">Xóa</a></button></td>
                            
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    }
